{
  "version": 2,
  "builds": [
    {
      "src": "UserServer/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "OwnerServer/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/user/(.*)",
      "dest": "UserServer/server.js"
    },
    {
      "src": "/owner/(.*)",
      "dest": "OwnerServer/index.js"
    }
  ]
}